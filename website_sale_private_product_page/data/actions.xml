<odoo>
    <record model="ir.actions.server" id="generate_tokens">
        <field name="name">Generate Private URL Token</field>
        <field name="model_id" ref="model_product_template" />
        <field name="binding_model_id" ref="model_product_template" />
        <field name="state">code</field>
        <field name="code">records.action_generate_private_url_token()</field>
    </record>

    <record model="ir.actions.server" id="clear_tokens">
        <field name="name">Clear Private URL Token</field>
        <field name="model_id" ref="model_product_template" />
        <field name="binding_model_id" ref="model_product_template" />
        <field name="state">code</field>
        <field name="code">records.action_clear_private_url_token()</field>
    </record>
</odoo>
